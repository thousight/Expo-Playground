{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "plugins": ["prettier", "jest"],
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "semicolon": "off",
    "ordered-imports": "off",
    "react/jsx-filename-extension": "off",
    "react/react-in-jsx-scope": "off"
  }
}
